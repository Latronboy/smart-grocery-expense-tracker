{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/(style\\.css|script\\.js)",
            "dest": "$1"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}